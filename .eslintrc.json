{
  "extends": "airbnb",
  "installedESLint": true,
  "plugins": [
    "react"
  ],
  "settings": {
    "import/parser": "babel-eslint",
    "import/resolver": {
      "webpack": {
        "config": "webpack/webpack.client.babel.js"
      }
    }
  },
  "parserOptions":{
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "browser": true,
    "node": true
  }
}
